{
  "NetAgent": {
    "PostProcessors": {
      "EnableLogging": true,
      "EnableOptimization": true
    },
    "Optimizer": {
      "Type": "prompt"
    },
    "Evaluator": {
      "Type": "llm"
    },
    "LLM": {
      "Providers": ["openai", "claude"],
      "OpenAI": {
        "ApiKey": "your-openai-api-key",
        "Model": "gpt-4",
        "Temperature": 0.7,
        "MaxTokens": 2000
      },
      "Claude": {
        "ApiKey": "your-anthropic-api-key",
        "Model": "claude-3-opus-20240229",
        "Temperature": 0.7,
        "MaxTokens": 2000
      },
      "DeepSeek": {
        "ApiKey": "",
        "Model": "deepseek-coder",
        "Temperature": 0.7,
        "MaxTokens": 2000
      },
      "Grok": {
        "ApiKey": "",
        "Model": "grok-1",
        "Temperature": 0.7,
        "MaxTokens": 2000
      }
    },
    "Planner": {
      "Type": "default",
      "MaxAttempts": 3,
      "AllowRetry": true
    },
    "Tools": {
      "Types": [ "websearch", "calculator" ],
      "EnableParallelExecution": true,
      "TimeoutSeconds": 30
    },
    "Context": {
      "Type": "default",
      "MaxHistoryItems": 10,
      "IncludeTimestamp": true
    },
    "Memory": {
      "Type": "inmemory",
      "MaxItems": 1000,
      "ExpirationMinutes": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
