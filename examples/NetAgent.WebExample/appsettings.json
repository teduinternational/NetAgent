{
  "NetAgent": {
    "PostProcessors": {
      "EnableLogging": true,
      "EnableOptimization": true
    },
    "Optimizer": {
      "Type": "prompt"
    },
    "Evaluator": {
      "Type": "llm"
    },
    "LLM": {
      "Provider": "openai",
      "OpenAI": {
        "ApiKey": "sk-xxx",
        "Model": "gpt-3.5-turbo"
      }
    },
    "Planner": {
      "Type": "default"
    },
    "Tools": {
      "Types": [ "datetime", "weather", "http" ]
    },
    "Context": {
      "Type": "default"
    },
    "Memory": {
      "Type": "inmemory"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
